
public class PrintArr {

	int[] arr = {2,5,3,1,4};
	
	void printAll(int[] arr) {
		for(int num : arr) {
			System.out.print(num + " ");
		}
		System.out.println();
	}
	
	void printSum(int[] arr) { // 합계
		int sum1 = 0;
		for(int sum : arr) {
			sum1 += sum;
		}
		System.out.println(sum1);
	}
	
	void printSort(int[] arr) { // 오름차순
		int x = 0;
		for(int i=0; i<arr.length; i++) {
			if(arr[i] < arr[i+1]) {
				x = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = x;
			}
			System.out.print(arr[i] + " ");
		}
	}
}
