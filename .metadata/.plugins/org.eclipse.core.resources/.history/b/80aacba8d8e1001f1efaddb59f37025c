
public class TriangleMain {

	public static void main(String[] args) {

		Triangle t = new Triangle();
		
		t.p[0].x = 10;
		t.p[0].y = 10;
		
		t.p[1].y = 20;
		t.p[1].y = 20;
		
		t.p[2].y = 30;
		t.p[2].y = 30;
		
		for(int i=0; i<t.p.length; i++) {
			t.p[i].x = 10;
			t.p[i].y = 20;
			System.out.println("x = " + t.p[i].x + "y = " + t.p[i].y);
		}
		
	}

}
