
public class DataMain {

	public static void main(String[] args) {

		Data d1 = new Data();
		d1.x = 10;
		System.out.println("변경전 x = " + d1.x);
		
		change(d1.x);
		System.out.println("변경 후 = " + d1.x);
	}

	static void change(int x) {
		x = 1000;
		System.out.println("change x = " + x );
	}
	
}
