
public class PrintArr {

	int[] arr = {2,5,3,1,4};
	
	void printAll(int[] arr) {
		for(int num : arr) {
			System.out.print(num + " ");
		}
	}
	
	void printSum(int[] arr) { // 합계
		int sum1 = 0;
		for(int sum : arr) {
			sum1 += sum;
		}
		System.out.println(sum1);
	}
	
	void printSort(int[] arr) { // 오름차순
		
	}
}
